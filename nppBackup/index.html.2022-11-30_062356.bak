<!DOCTYPE html>
<html>
	<style>
		html, body {
		  margin: 5px;
		  width: 100%;
		  height: 100%;
		}

		object { 
		  width: 100%;
		  height: 100%;
		}
	</style>
	<body bgcolor="#212148">
		<object data="calendar.svg" type="image/svg+xml" id="calendar_obj" width="100%" height="100%" style="margin: auto"></object>
		<script>
			var calendarContent;
			var windows;
			var openedWindow;
			var openedWindowBBox;
			
			window.onload = function() {
				calendarContent = document.getElementById("calendar_obj").contentDocument;
				openedWindow = calendarContent.querySelector('[id="Opened_Window"]');
				
				<!-- hack to get BBox of openedWindow -->
				openedWindow.classList.display = "block";
				openedWindowBBox = openedWindow.getBBox();
				openedWindow.style.display = "none";
				
				windows = calendarContent.querySelectorAll('[id^="Window"]');
				windows.forEach(function(currentValue, currentIndex, listObj) {
					currentValue.addEventListener("click", OnWindowClicked, false);
				});
			}
			
			function OnWindowClicked(e){
				var clickedWindowBBox = e.currentTarget.getBBox();
				
				<!-- var openedClone = openedWindow.cloneNode(true); -->
				<!-- openedClone.style.display = "block"; -->
				<!-- calendarContent.appendChild(openedClone); -->
				
				var deltaX = clickedWindowBBox.x - openedWindowBBox.x;
				var deltaY = clickedWindowBBox.y - openedWindowBBox.y;
				var transformValue = "translate(" + deltaX + "," + deltaY + ")";
				
				openedWindow.setAttribute('transform', transformValue);
				openedWindowBBox = openedWindow.getBBox();
			}
			
			
		</script>
	</body>
</html>