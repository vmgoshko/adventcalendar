<!DOCTYPE html>
<html>
	<style>
		html, body {
		  margin: 5px;
		  width: 100%;
		  height: 100%;
		}

		object { 
		  width: 100%;
		  height: 100%;
		}
	</style>
	<body bgcolor="#212148">
		<object data="calendar.svg" type="image/svg+xml" id="calendar_obj" width="100%" height="100%" style="margin: auto"></object>
		<script>
			var calendarContent;
			var windows;
			var openedWindow;
			var openedWindowBox;
			
			window.onload = function() {
				calendarContent = document.getElementById("calendar_obj").contentDocument;
				windows = calendarContent.querySelectorAll('[id^="Window"]');
				openedWindow = calendarContent.querySelector('[id="Window_Opened"]');
				
				// hack to get BBox of openedWindow
				openedWindow.style.display = "block";
				openedWindowBBox = openedWindow.getBBox();
				openedWindows.style.display = "none";
				
				windows.forEach(function(currentValue, currentIndex, listObj) {
					currentValue.addEventListener("click", OnWindowClicked, false);
				});
			}
			
			function OnWindowClicked(e){
				var clickedWindowBBox = e.currentTarget.getBBox();
				var newElement = document.createElementNS("http://www.w3.org/2000/svg", 'g'); //Create a path in SVG's namespace
			}
			
			
		</script>
	</body>
</html>