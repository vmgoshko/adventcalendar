<!DOCTYPE html>
<html>
	<body bgcolor="#212148">
		<object data="calendar.svg" type="image/svg+xml" id="calendar_obj" height="100%"></object>
		<script>
			var calendarContent 
			window.onload = function() {
				calendarContent = document.getElementById("calendar_obj").contentDocument;
				var windows = calendarContent.querySelectorAll('[id^="Window"]');
				
				windows.forEach(function(currentValue, currentIndex, listObj) {
					currentValue.addEventListener("click", OnWindowClicked, false);
				});
			}
			
			function OnWindowClicked(e){
				e.currentTarget.classList.add("st0");
			}
		</script>
	</body>
</html>